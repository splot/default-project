{
    "name": "splot/default-project",
    "description": "Default application project skeleton for Splot Framework.",
    "keywords": ["splot", "application", "skeleton"],
    "license": "MIT",
    "authors": [
        {
            "name": "MichaÅ‚ Dudek",
            "email": "michal@michaldudek.pl"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "http://packagist.org/"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "splot/framework": "0.8.*@dev",
        "splot/framework-extra-module": "dev-master",
        "splot/assets-module": "dev-master",
        "splot/twig-module": ">=0.8",
        "splot/knit-module": "dev-master",
        "splot/devtools-module": "dev-master",
        "splot/weblog-module": ">=0.8"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "scripts": {
        "post-install-cmd": [
            "Splot\\AssetsModule\\Composer\\ScriptHandler::installAssets",
            "Splot\\DevToolsModule\\Composer\\ScriptHandler::clearCache"
        ],
        "post-update-cmd": [
            "Splot\\AssetsModule\\Composer\\ScriptHandler::installAssets",
            "Splot\\DevToolsModule\\Composer\\ScriptHandler::clearCache"
        ]
    }
}